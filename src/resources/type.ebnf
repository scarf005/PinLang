(* type
    =
    | 'bool'
    | 'cstr'
    | 'unit'
    ; *)

type_specifier
    =
        int_type
    ;

int_type
    =
    | 'int'
    | 'i8'
    | 'i16'
    | 'i32'
    | 'i64'
    | 'uint'
    | 'u8'
    | 'u16'
    | 'u32'
    | 'u64'
    | 'f32'
    | 'f64'
    ;

# 숫자 정의
number::Number
    =
    /\d+/
    ;