@@grammar::Pin
#@@parseinfo :: True

start
    =
    code_block
    $
    ;

code_block
    =
    @+:instruction {END @+:instruction}
    ;

instruction
    =
    | expression
    | statement
    ;

#include :: "pin/grammar/statement/statement.ebnf"
#include :: "pin/grammar/expression/expression.ebnf"
#include :: "pin/grammar/token/token.ebnf"

